{
  "name": "MyApplication",
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "build": "webpack",
    "watch": "webpack --watch",
    "eslint": "eslint index.js server.js",
    "stylelint": "stylelint index.scss",
    "prepublishOnly": "npm run test && rm ./dist/* && npm run build"
  },
  "dependencies": {
    "@osjs/widgets": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@osjs/eslint-config": "^1.0.2",
    "@osjs/stylelint-config": "^1.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.0.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "extends": "@osjs/stylelint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "osjs": {
    "type": "package"
  }
}
